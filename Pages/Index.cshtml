@page
@model IndexModel
@{
    ViewData["Title"] = "Acid december";
}

<div class="text-center">
    <h1>ACID DECEMBER</h1>
    <p>It's so dark. Who can help us? You can! Please email acid@datapop.se with your track, name, picture if you want,
        link to your music. <3 </p>

    @foreach (var song in Model.Songs)
    {
                <p>
                    <img class="float-start img-thumbnail"
                        src="https://storage.googleapis.com/acid-december2012/2023/@song.ImageLink" />
                <h2>@song.Title</h2>
                <h3>@song.Artist</h3>
            @song.Artistlink
        </p>
       }

